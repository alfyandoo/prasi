{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
  },
  "include": [
    "./src/**/*",
    "./jsx-*.ts", "../../frontend/src/utils/react/use-local.ts", "../../frontend/src/utils/react/use-global.ts", "../../frontend/src/utils/react/define-react.ts", "../../frontend/src/utils/react/define-window.ts", "../../frontend/src/utils/react/page.ts",
  ]
}
{
  "name": "prasi",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "bun run --silent backend/init/dev-bun.ts",
    "dev": "bun run --hot --silent backend/init/dev.ts",
    "prep": "bun run --silent backend/init/prep.ts",
    "prod": "bun run --silent backend/init/prod.ts",
    "db": "cd backend/db && bun prisma db pull && bun prisma generate && cd ../.."
  },
  "workspaces": [
    "frontend",
    "backend/*"
  ],
  "trustedDependencies": [
    "@prisma/client",
    "@prisma/engines",
    "@swc/core",
    "esbuild",
    "leveldown",
    "msgpackr-extract",
    "prisma"
  ],
  "dependencies": {
    "@types/bun": "^1.1.13",
    "@types/lodash.transform": "^4.6.9",
    "@types/lodash.uniq": "^4.5.9",
    "lodash.transform": "^4.6.0",
    "lodash.uniq": "^4.5.0"
  }
}

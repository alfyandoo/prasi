{
  "compilerOptions": {
    "paths": {
      "crdt/*": ["./src/nova/ed/crdt/*"],
      "utils/*": ["./src/utils/*"],
      "base/*": ["./src/base/*"],
      "logic/*": ["./src/nova/ed/logic/*"],
      "popup/*": ["./src/nova/ed/popup/*"],
      "mode-page/*": ["./src/nova/ed/mode/page/*"],
      "mode-query/*": ["./src/nova/ed/mode/query/*"],
      "prod/*": ["./src/nova/prod/*"],
      "expr*": ["./src/nova/ed/popup/script/expr"],
      "vi/*": ["./src/nova/vi/*"],
      "prasi-prisma": ["../node_modules/.prisma/client/index.d.ts"]
    },
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": true
  },
  "include": ["src"],
  "exclude": ["src/nova/ed/cprasi/prasi-typings-generated.d.ts"]
}
